<% content_for :sub_nav do %>
  <li><%= link_to 'Admin Listing', admin_admins_path %></li>
<% end %>

<h2>Editing admin</h2>

<% form_for([:admin,@admin]) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>