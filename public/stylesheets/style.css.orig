  padding-top: 20px;
  margin-top: 10px;
  border-top: 1px solid #457086;
}
.promotion .buying_details .first_deal{
  margin-top: 0;
  padding-top: 0;
  border-top: none;
}
.promotion .buying_details .multi_deal .deal_checkbox{
  float: left;
  margin-right: 5px;
}
.promotion .buying_details .multi_deal .deal_checkbox .checkbox{
	width: 33px;
	height: 36px;
	padding: 0 5px 0 0;
	background: url(/images/checkbox.png) no-repeat;
	cursor: pointer;
	display: block;
	clear: left;
	float: left;
}

.promotion .buying_details .multi_deal .pricing_info{
  float: left;
  width: 240px;
}
.promotion .buying_details .multi_deal .pricing_info dl{
  margin-bottom: 10px;
  float: left;
}
.promotion .buying_details .multi_deal .pricing_info dl dt{
  font-size: 13px;
  color: #678aa1;
  text-transform: uppercase;
}
.promotion .buying_details .multi_deal .pricing_info dl dd{
  font-size: 16px;
  font-weight: bold;
  color: #fff;
  margin-right: 10px;
}
.promotion .buying_details .multi_deal .pricing_info h3{
  color: #aac5db;
  font-size: 150%;
  font-weight: normal;
  font-family: 'Myriad Pro', Myriad, Helvetica, sans-serif;
  text-align: left;
}
.promotion .buying_details .multi_deal .pricing_info h3 label{
  cursor: pointer;
}
.promotion .buying_details .multi_deal .pricing_info dl dd.early_bird_discount{
  color: #099aff;
  margin-right: 7px;
}
.promotion .buying_details .multi_deal .pricing_info dl dd.early_bird_discount span{
  text-decoration: line-through;
  color: #989898;
  font-size: 85%;
  font-weight: normal;
}


.promotion .buying_details .checkout_buttons{
  margin-top: 20px;
}
.promotion .buying_details .checkout_buttons .primary{
  margin: 0;
  font-size: 200%;
  font-weight: 600;
  padding: 7px 17px;
  text-shadow: 0px 0px 1px #000;
  -moz-border-radius: 7px; 
  -webkit-border-radius: 7px; 
  -moz-box-shadow: 0px 0px 7px #000;
  -webkit-box-shadow: 0px 0px 7px #000;
}
.promotion .buying_details .checkout_buttons .buy_as_gift{
  font-size: 16px;
  color: #de9fdc;
  border: none;
  cursor: pointer;
  padding: 5px 0 5px 18px;
  background: transparent url(/images/promo_gift.png) no-repeat center left;
}

.promotion .promotion_image{
  float: right;
}
.promotion .promotion_montage{
  text-align: center;
}
.promotion .promotion_image img,
.promotion .promotion_montage img{
  border: 2px solid #9cafb9;
}
.promotion .promotion_image .social_buttons{
  margin: 10px 0 0 25px;
}

.promotion .promotion_image .social_buttons .facebook_like{
  width: 83px;
  height: 20px;
  overflow: hidden;
  float: left;
  margin-right: 27px;
}
.promotion .promotion_image .social_buttons .twitter_share{
  float: left;
  margin-right: 35px;
}
.promotion .promotion_image .social_buttons .facebook_share{
  float: left;
  padding-top: 1px;
}


.promotion .promotion_info_strip{
  font-size: 15px;
  line-height: 0;
  padding: 9px 11px;
  margin-bottom: 40px;
  background: #082534;
  color: #aac5db;
  -moz-border-radius: 7px; 
  -webkit-border-radius: 7px; 
}
.promotion .promotion_info_strip strong{
  color: #fff;
}
.promotion .promotion_info_strip .purchases{
  float: left;
  text-align: left;
  text-transform: uppercase;
  margin-right: 15px;
  padding-top: 16px;
  padding-left: 5px;
}
.promotion .promotion_info_strip .deal_is_on,
.promotion .promotion_info_strip .early_bird{
  float: left;
  text-align: left;
  margin-right: 10px;
  margin-top: 1px;
  padding: 5px 5px 3px 20px;
  border: 1px solid #ffa509;
  color: #ffa509;
  line-height: 1.25em;
  font-size: 90%;
}
.promotion .promotion_info_strip .deal_is_on{
  background: transparent url(/images/promo_thumbs_up.png) no-repeat 3px 3px;
}
.promotion .promotion_info_strip .early_bird{
  border-color: #099aff;
  color: #099aff;
  background: transparent url(/images/promo_early_bird.png) no-repeat 3px 5px;
}
.promotion .promotion_info_strip .time_left{
  float: right;
  text-align: right;
  text-transform: uppercase;
  margin-left: 10px;
  margin-right: 0;
  padding-top: 16px;
  vertical-align: top;
}
.promotion .promotion_info_strip .reminded{
  font-size: 85%;
  margin-top: 1px;
}
.promotion .reminder_container{
  float: right;
  text-align: right;
  margin-left: 10px;
  margin-right: 0;
}
.promotion .reminder_container .reminded{
  border: 1px solid #51B757;
  color: #51B757;
  padding: 5px 5px 3px;
  line-height: 1.25em;
}
.promotion .promotion_info_strip .almost_over strong{
  color: #C72E3C;
}
.promotion .reminder_container .secondary{
  background: transparent url(/images/promo_reminder_bg.png) repeat-x center center;
  font-size: 85%;
  font-weight: bold;
  color: #fff;
  padding: 4px 8px;
  text-shadow: 0px 0px 2px #000;
  -moz-border-radius: 5px; 
  -webkit-border-radius: 5px;
  -moz-box-shadow: 0px 0px 4px #000;
  -webkit-box-shadow: 0px 0px 4px #000;
}
.promotion .last_chance{
  background: transparent url(/images/promo_hourglass.png) no-repeat 4px 3px;
  border: 1px solid #E03038;
  color: #E03038;
  float: left;
  line-height: 1.25em;
  padding: 5px 5px 3px 19px;
  text-align: left;
}

.promotion .promotion_tabs .tab{
  font-size: 120%;
  padding: 10px 15px 7px;
  margin-right: 10px;
  background: #135e92 url(/images/promo_tab_bg.png) repeat-x bottom center;
  text-transform: uppercase;
  color: #c6e0ed;
  -moz-border-radius: 10px 10px 0 0; 
  -webkit-border-radius: 10px 10px 0 0; 
}
.promotion .promotion_tabs .active_tab{
  background: #082534;
  color: #fff;
  text-decoration: none;
  cursor: default;
}
.promotion .tab_content{
  font-size: 120%;
  color: #ddd;
  background: #082534;
  padding: 25px 15px 15px 20px;
  margin: 8px 0 20px;
  -moz-border-radius: 0 15px 15px 15px; 
  -webkit-border-radius: 0 15px 15px 15px; 
}

.promotion .tab_content .promotion_map{
  float: right;
  border: 5px solid #fff;
}
.promotion .tab_content .promotion_summary{
  float: left;
  width: 275px;
  margin-right: 15px;
}
.promotion .tab_content ul{
  list-style-type: disc;
  list-style-position: outside;
  margin-left: 15px;
}
.promotion .tab_content ul li{
  margin-bottom: 10px;
  line-height: 1.2em;
}
.promotion .tab_content address{
  text-align: center;
  margin: 0;
  padding: 0;
  color: #fff;
}

.promotion .tab_content p{
  color: #ddd;
  line-height:130%;
  margin-bottom: 10px;
}


/* more great deals */
.promotion_index .promotion{
  -moz-border-radius: 10px;
  -webkit-border-radius: 10px;
  clear: both;
  position: relative;
}
.promotion_index .promotion h2{
  font-size: 175%;
  text-align: center;
  margin-bottom: 20px;
}
.promotion_index .promotion h2.mutli_deal_header{
  margin-right: 56px;
}

.promotion_index .promotion .main_details{
  margin-bottom: 0;
}
.promotion_index .promotion .buying_details{
  width: 200px;
  text-align: center;
  float: left;
  margin-bottom: 18px;
}
.promotion_index .promotion .buying_details .single_deal{
  margin-bottom: 0;
}
.promotion_index .promotion .buying_details .single_deal dl{
  clear: both;
  display: block;
  margin-bottom: 0;
}
.promotion_index .promotion .buying_details .single_deal dl dt{
  font-size: 18px;
  padding: 10px 35px 10px 0;
  background: transparent url(/images/promo_arrow_small.png) no-repeat center right;
  width: 90px;
}
.promotion_index .promotion .buying_details .single_deal dl dd{
  font-size: 20px;
  padding: 10px 0 10px 5px;
}

.promotion_index .promotion .promotion_image{
  float: left;
  margin-right: 20px;
}
.promotion_index .promotion .promotion_image img{
  width: 259px;
}

.promotion_index .promotion .promotion_info_strip{
  float: left;
  font-size: 13px;
  padding: 18px 15px;
  margin-bottom: 0px;
  width: 290px;
  -moz-border-radius: 0px; 
  -webkit-border-radius: 0px; 
}
.promotion_index .promotion .promotion_info_strip .purchases{
  padding-top: 3px;
}
.promotion_index .promotion .promotion_info_strip .time_left{
  padding-top: 3px;
}
.promotion_index .promotion .reminder_container{
  margin-left: 0;
  margin-right: 0;
  position: relative;
  top: 20px;
  right: 10px;
}
.promotion_index .promotion .reminder_container .reminded{
  padding: 5px 5px 3px;
  line-height: 1.25em;
}
.promotion_index .promotion .reminder_container .secondary{
  font-size: 100%;
}

.promotion_index .promotion a.multi_deal_badge{
  font-size: 13px;
  font-weight: bold;
  line-height: 1em;
  color: #fff;
  text-shadow: 0px 0px 1px #333;
  text-align: center;
  position: absolute;
  right: -23px;
  top: -20px;
  background: transparent url(/images/promo_multi_deal_badge.png) no-repeat center center;
  width: 100px;
  padding: 40px 0 32px;
}

/* safari fixes */
@media screen and (-webkit-min-device-pixel-ratio:0) {
    .promotion .promotion_info_strip { padding: 8px 11px 10px; }
    .promotion .promotion_info_strip .purchases { padding-top: 14px; }
    .promotion .promotion_info_strip .time_left { padding-top: 14px; }
    .promotion .promotion_info_strip .reminded { padding-top: 4px; padding-bottom: 4px; margin-top: 1px;}
    .promotion .promotion_info_strip .early_bird { padding-top: 4px; padding-bottom: 4px; margin-top: 1px;}
    .promotion .promotion_info_strip .deal_is_on { padding-top: 4px; padding-bottom: 4px; margin-top: 1px;}
